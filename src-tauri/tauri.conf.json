{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Template Nuxt Tauri Vuetify",
  "version": "0.1.0",
  "identifier": "com.mathis-beal.template",
  "build": {
    "frontendDist": "../.output/public",
    "devUrl": "http://localhost:3000",
    "beforeDevCommand": "pnpm nuxt",
    "beforeBuildCommand": "pnpm generate"
  },
  "app": {
    "windows": [
      {
        "title": "Template App",
        "width": 800,
        "height": 600,
        "resizable": true,
        "maximizable": true,
        "maximized": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
